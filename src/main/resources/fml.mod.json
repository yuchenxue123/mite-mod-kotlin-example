{
  "schemaVersion": 1,
  "id": "example",
  "name": "Example Mod",
  "version": "${version}",
  "description": "An example mod with kotlin.",
  "authors": [
    "yuchenxue"
  ],
  "depends": {
    "fishmodloader": ">=3.4.1",
    "kotlin-language": ">=1.0.1",
    "kawakazelib": ">=1.0"
  },
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "com.example.ExampleMod"
      }
    ]
  },
  "mixins": [
    "example.mixins.json"
  ],
  "accessWidener": "example.accesswidener"
}